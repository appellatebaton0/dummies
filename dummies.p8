function _update()
	if (btn(0)) then x=x-speed end
	if (btn(1)) then x=x+speed end
	if (btn(2)) then y=y-speed end
	if (btn(3)) then y=y+speed end
end

function _draw()
	cls(6)
	circfill(x,y,7,14)
	print("quite explicable",30,30,00)
	print("does this work?",30,36,00)
end
